<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycalifornia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_flag"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HotelActivity" />
        <activity android:name=".BeachActivity" />
        <activity android:name=".CityActivity" />
        <activity android:name=".MountainsActivity" />
    </application>

</manifest>